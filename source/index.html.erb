  <% data.charset.each do | id, this | %>
    <div class="flex bt mt3">
      <div class="w-30">
        <p><%= id.capitalize %></p>
      </div>
      <div class="w-70">
      <% this.each do |this| %>
        <div class="glyph w-50 w-25-ns w-10-l fl charset">
          <div class="aspect-ratio--1x1 aspect-ratio">
            <div class="aspect-ratio--object expand overflow-hidden">
              <div class="f4 f3-ns list ma0 pa0 relative w-100 h-100 flex justify-center items-center">
                <% if this.is_a?(Hash) %>
                  <ul class="list ma0 pa0 expand-list w-100 h-100 relative">
                  <li class="flex items-center justify-center w-100 h-100"><%= this['char'] + this['char'].mb_chars.downcase %></li>
                  <% if this.key?('dia') %>
                    <% this['dia'].each do | dia | %>
                       <li class="flex items-center justify-center w-100 h-100"><%= dia + dia.mb_chars.downcase %></li>
                    <% end %>
                  <% end %>
                  </ul>
                <% elsif %>
                  <%= this %>
                <% end %>
              </div>
            </div>
          </div>
        </div>
      <% end %>
      </div>
    </div>
  <% end %>


</div>
